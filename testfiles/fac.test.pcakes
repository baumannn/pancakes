print("factorial recursivo\n\n");

print(factorial_rec(8));


int factorial_rec (int num) {
	
	if(num == 0) {
		return 1;
	}

	return num * factorial_rec (num-1);
}


print("factorial iterativo\n\n");
int num = 5;
int fact = 1;
int i = 1;
while( i < num + 1) {

	fact = fact*i;

	i = i + 1;
}

print(fact);
