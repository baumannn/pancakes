print("Arreglo de dos dimension, arreglo[i][j] = i * j: \n\n");


int [10][10] arr;

i = 0;
int j;
while(i < 10){

	j = 0;
	while(j < 10) {

		arr[i][j] = i*j;


		j = j + 1;
	}




	i = i + 1;
}


i = 0;
while(i < 10){

	j = 0;
	while(j < 10) {

		print(arr[i][j]);
		print(" ");

		j = j + 1;
	}

	print("\n");


	i = i + 1;
}



